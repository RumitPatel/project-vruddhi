<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/niramay_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:name=".App"
        android:theme="@style/Theme.Androidgeocode"
        tools:targetApi="31">
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.ResetPasswordActivity" />
        <activity android:name=".activities.ResetPasswordSentActivity" />
        <activity android:name=".activities.HomeActivity" />
        <activity android:name=".activities.PregnantWomanListActivity" />
        <activity android:name=".activities.PregnantWomanScreeningActivity" />
        <activity android:name=".activities.PregnantWomanRegistrationActivity" />
        <activity android:name=".activities.PregnantWomanServicesActivity" />
        <activity android:name=".activities.PregnantWomanExitActivity" />
    </application>

</manifest>